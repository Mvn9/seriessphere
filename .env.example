# .env.example - SeriesSphere
# Replace placeholders with real values in your local .env.local (do NOT commit .env.local)

# App URLs
NEXT_PUBLIC_VERCEL_URL=https://seriessphere.vercel.app
NEXTAUTH_URL=http://localhost:9002

# Genkit / Google AI
GENKIT_API_KEY=your_genkit_api_key_here
GENKIT_ENDPOINT=https://api.genkit.example/v1/flows/run
GOOGLE_API_KEY=your_google_api_key_here

# Client-side base URL (if used)
NEXT_PUBLIC_API_BASE_URL=http://localhost:9002/api

# Database & Auth (example placeholders)
DB_URL=mysql://root:password@localhost:3306/seriessphere
JWT_SECRET=change_this_dev_secret

# Optional: development port
PORT=9002
